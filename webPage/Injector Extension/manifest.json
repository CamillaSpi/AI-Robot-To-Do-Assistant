{
    "name": "Injector",
    "description": "Inject Your Custom Javascript",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "http://*.localhost/webPage/*",
                "https://*.google.com/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ]
}